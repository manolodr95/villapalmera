<odoo>
    <data noupdate="1">

        <record id="seq_condo_contract" model="ir.sequence">
            <field name="name">Condo Contract sequence</field>
            <field name="code">seq.condo.contract</field>
            <field name="prefix">CC/%(range_year)s/</field>
            <field name="padding">3</field>
            <field name="implementation">no_gap</field>
        </record>

        <!--************************************************************************-->

        <record forcecreate="True" id="decimal_charge" model="decimal.precision">
            <field name="name">charge</field>
            <field name="digits">4</field>
        </record>

    </data>
</odoo>
